<?xml version="1.0" encoding="utf-8" ?>
<rss version="2.0"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:content="http://purl.org/rss/1.0/modules/content/">
  <channel>
    <title>{{title}}</title>
    <link>http://pqrs.org/</link>
    <description>{{title}}</description>
    <dc:creator>Takayama Fumihiko</dc:creator>
    {{#items}}
    <item>
      <title>{{item_title_l10n}}</title>
      <link>{{link}}</link>
      <description>{{body_l10n}}</description>
      <dc:creator>Takayama Fumihiko</dc:creator>
      <pubDate>{{#format_date_rss}}{{updated}}{{/format_date_rss}}</pubDate>
    </item>
    {{/items}}
  </channel>
</rss>
