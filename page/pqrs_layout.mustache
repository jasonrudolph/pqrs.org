<!DOCTYPE html>
<html lang="{{language}}">
  <head>
    <meta charset="utf-8" />
    <title>{{title}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Le styles -->
    <link href="/assets/bootstrap/v3.0.0-rc1/css/bootstrap.min.css" rel="stylesheet" />
    <style>
      body { padding-top: 70px; }
    </style>
    <link href="/assets/bootstrap-glyphicons/v1.0.0/css/bootstrap-glyphicons.css" rel="stylesheet" />
    <link href="/assets/bootstrap-lightbox/bootstrap-lightbox.min.css" rel="stylesheet" />
    {{#enable_prettify}}
      <link href="/assets/bootstrap-prettify/prettify.css" rel="stylesheet" type="text/css"/>
    {{/enable_prettify}}
    <link href="/css/pqrs.css?1375424190" rel="stylesheet" />

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="/assets/html5/html5shiv.js"></script>
    <script src="/assets/html5/respond.min.js"></script>
    <![endif]-->

    <!-- Le fav and touch icons -->
    <link rel="shortcut icon" href="/favicon.ico" />
  </head>

  <body data-spy="scroll" data-target="#bs-sidebar">
    <div id="navbar" class="navbar navbar-fixed-top">
      <div class="container">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/index.html.{{language}}">pqrs.org</a>
        <div class="nav-collapse collapse">
          <ul class="nav navbar-nav">
            {{#navs_left}}
              {{#dropdown}}
                <li class="{{nav_class}}">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                    {{name_l10n}}
                    <b class="caret"></b>
                  </a>
                  <ul class="dropdown-menu">
                    {{#items}}
                      <li {{#dropdown_nav_class}}class="{{dropdown_nav_class}}"{{/dropdown_nav_class}}>
                        <a href="{{href}}.{{language}}">{{name_l10n}}</a>
                      </li>
                    {{/items}}
                  </ul>
                </li>
              {{/dropdown}}
              {{^dropdown}}
                <li {{#nav_class}}class={{nav_class}}{{/nav_class}}>
                  <a href="{{href}}.{{language}}">{{name_l10n}}</a>
                </li>
              {{/dropdown}}
            {{/navs_left}}
          </ul>
          <ul class="nav navbar-nav pull-right">
            {{#navs_right}}
              <li {{#nav_class}}class={{nav_class}}{{/nav_class}}>
                <a href="{{href}}.{{language}}">{{name_l10n}}</a>
              </li>
            {{/navs_right}}
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                Language
                <b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><a href="{{path}}.en">English</a></li>
                <li><a href="{{path}}.ja">Japanese</a></li>
              </ul>
            </li>
          </ul>
        </div>
      </div>
    </div>

    {{{body}}}

    <footer class="footer">
      <div class="container">
        <p class="pull-right"><a href="#">Back to top</a></p>
      </div>
    </footer>

    <!-- Le javascript -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="/assets/jquery/jquery-2.0.3.min.js"></script>
    <script src="/assets/bootstrap/v3.0.0-rc1/js/bootstrap.min.js"></script>
    <script src="/assets/bootstrap-lightbox/bootstrap-lightbox.min.js"></script>
    {{#enable_prettify}}
      <script src="/assets/google-code-prettify/prettify.js" type="text/javascript"></script>
      <script type="text/javascript">jQuery(function(){ prettyPrint(); });</script>
    {{/enable_prettify}}
    {{#enable_jquery_collapse_js}}
      <script type="text/javascript" src="/assets/jquery/jquery.collapse.js"></script>
      <script type="text/javascript">jQuery(function(){ jQuery('#collapser').jqcollapse(); });</script>
    {{/enable_jquery_collapse_js}}
    <script src="/js/pqrs.js?1375492917"></script>
  </body>
</html>
